{
  "name": "memo-monorepo-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:front": "./node_modules/.bin/env-cmd -e development nx serve front",
    "start:api": "./node_modules/.bin/env-cmd -e development nx serve api",
    "start:api:e2e": "./node_modules/.bin/env-cmd -e e2e prisma migrate reset --schema=./apps/api/src/infrastructure/prisma/schema.prisma --force --skip-generate && ./node_modules/.bin/env-cmd -e e2e nx serve api",
    "build:api": "nx build api",
    "build:front": "nx build front",
    "export:development": "./node_modules/.bin/env-cmd -e development nx export front --skip-nx-cache && mv ./dist/apps/front/public/ ./dist/apps/front/exported/",
    "export:staging": "./node_modules/.bin/env-cmd -e staging nx export front && mv ./dist/apps/front/public/ ./dist/apps/front/exported/",
    "export:production": "./node_modules/.bin/env-cmd -e production nx export front && mv ./dist/apps/front/public/ ./dist/apps/front/exported/",
    "test:front": "./node_modules/.bin/env-cmd -e test nx test front",
    "test:api": "./node_modules/.bin/env-cmd -e test nx test api --runInBand --forceExit --detectOpenHandles",
    "test:api:separately": "./node_modules/.bin/env-cmd -e test jest --runInBand --forceExit --detectOpenHandles",
    "e2e": "./node_modules/.bin/env-cmd -e e2e nx e2e front-e2e --watch --prod",
    "e2e:headress": "./node_modules/.bin/env-cmd -e e2e nx e2e front-e2e --prod",
    "e2e:ci": "start-server-and-test start:api:e2e http-get://localhost:3200/graphql e2e:headress",
    "prisma:gen": "prisma generate --schema=./apps/api/src/infrastructure/prisma/schema.prisma",
    "migrate": "./node_modules/.bin/env-cmd -e development prisma migrate deploy --schema=./apps/api/src/infrastructure/prisma/schema.prisma && ./node_modules/.bin/env-cmd -e test prisma migrate deploy --schema=./apps/api/src/infrastructure/prisma/schema.prisma && prisma generate --schema=./apps/api/src/infrastructure/prisma/schema.prisma",
    "migrate:dev": "prisma format --schema=./apps/api/src/infrastructure/prisma/schema.prisma && ./node_modules/.bin/env-cmd -e development prisma migrate dev --schema=./apps/api/src/infrastructure/prisma/schema.prisma",
    "migrate:reset": "./node_modules/.bin/env-cmd -e development prisma migrate reset --schema=./apps/api/src/infrastructure/prisma/schema.prisma && ./node_modules/.bin/env-cmd -e test prisma migrate reset --schema=./apps/api/src/infrastructure/prisma/schema.prisma --force --skip-generate --skip-seed",
    "migrate:test": "./node_modules/.bin/env-cmd -e test prisma migrate deploy --schema=./apps/api/src/infrastructure/prisma/schema.prisma",
    "seed": "./node_modules/.bin/env-cmd -e development prisma db seed",
    "commit": "scripts/commit_message.sh",
    "branch": "scripts/checkout_branch.sh",
    "revert:version": "scripts/revert_to_target_version.sh",
    "lint": "nx run-many --all --target=lint",
    "format:sql": "prettier --write apps/api/src/infrastructure/big-query/sql/**",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:ci": "start-server-and-test start:api http-get://localhost:3200/graphql codegen"
  },
  "private": true,
  "dependencies": {
    "@aws-crypto/sha256-browser": "^2.0.2",
    "@chakra-ui/react": "^2.2.4",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@nestjs/apollo": "^10.0.19",
    "@nestjs/common": "9.0.8",
    "@nestjs/core": "9.0.8",
    "@nestjs/graphql": "^10.0.20",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "9.0.8",
    "@prisma/client": "^4.5.0",
    "@tanstack/react-table": "^8.7.6",
    "@urql/exchange-auth": "^1.0.0",
    "apollo-server-express": "^3.10.0",
    "axios": "^1.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.6.5",
    "env-cmd": "^10.1.0",
    "framer-motion": "^6",
    "google-auth-library": "^8.1.1",
    "graphql": "^16.5.0",
    "graphql-query-complexity": "^0.11.0",
    "graphql-type-json": "^0.3.2",
    "next": "13.1.1",
    "passport": "^0.6.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.34.2",
    "react-icons": "^4.4.0",
    "recoil": "^0.7.6",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "ts-morph": "^15.1.0",
    "tslib": "^2.3.0",
    "urql": "^3.0.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "@graphql-codegen/cli": "2.11.3",
    "@graphql-codegen/introspection": "2.2.0",
    "@graphql-codegen/typescript": "2.7.2",
    "@graphql-codegen/typescript-operations": "2.5.2",
    "@graphql-codegen/typescript-urql": "^3.6.4",
    "@graphql-codegen/urql-introspection": "^2.2.1",
    "@nestjs/schematics": "9.0.1",
    "@nestjs/testing": "9.0.8",
    "@nrwl/cli": "15.6.3",
    "@nrwl/cypress": "15.6.3",
    "@nrwl/eslint-plugin-nx": "15.6.3",
    "@nrwl/jest": "15.6.3",
    "@nrwl/js": "15.6.3",
    "@nrwl/linter": "15.6.3",
    "@nrwl/nest": "15.6.3",
    "@nrwl/next": "15.6.3",
    "@nrwl/node": "15.6.3",
    "@nrwl/react": "15.6.3",
    "@nrwl/web": "15.6.3",
    "@nrwl/workspace": "15.6.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@svgr/webpack": "^6.1.2",
    "@testing-library/react": "13.4.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/jest": "28.1.8",
    "@types/node": "18.11.9",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "5.40.0",
    "@typescript-eslint/parser": "5.40.0",
    "babel-jest": "28.1.3",
    "css-loader": "^6.4.0",
    "cypress": "^12.2.0",
    "eslint": "^8.20.0",
    "eslint-config-next": "13.1.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-eslint-plugin": "^5.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "28.1.3",
    "jest-environment-jsdom": "^29.2.0",
    "next-router-mock": "^0.9.1",
    "mysql2": "^3.1.0",
    "nx": "15.6.3",
    "passport-custom": "^1.1.1",
    "prettier": "^2.7.1",
    "prettier-plugin-sql": "^0.12.1",
    "prisma": "^4.5.0",
    "prisma-nestjs-graphql": "^17.0.0",
    "react-refresh": "^0.10.0",
    "react-test-renderer": "18.2.0",
    "start-server-and-test": "^1.14.0",
    "style-loader": "^3.3.0",
    "stylus": "^0.55.0",
    "stylus-loader": "^7.1.0",
    "supertest": "^6.2.4",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "4.8.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-merge": "^5.8.0"
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register -P apps/api/tsconfig.json --compiler-options {\"module\":\"CommonJS\"} apps/api/src/infrastructure/prisma/seed.ts"
  }
}